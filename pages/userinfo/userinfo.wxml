<!--pages/userinfo/userinfo.wxml-->
<view class='userinfo-top' style="background-image:url('/images/userinfo/bj.jpg');background-size: cover">

  <navigator url="/pages/setting/setting" class="account">
    <image src='/images/seting/设置.png'></image>
    <!-- <text>账号管理</text> -->
  </navigator>


  <view class='image'>
    <image src="/images/userinfo/tx.png" class='tx' />

    <view class='nickName'>{{userInfo.nickName}}</view>
  </view>

  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">我的订单</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">全部订单</view>
    </navigator>
  </view>

  <view class="navs">
    <block wx:for-items="{{orderItems}}" wx:key="name">
      
        <view  class='nav-item' catchtap="toOrder" data-type="{{item.name}}" data-typeid="{{item.typeId}}" data-url='{{item.url}}'>
         
          <image src="{{item.imageurl}}" class="nav-image" />
          <text>{{item.name}}</text>
        
        </view>
     
    </block>
  </view>


  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">待付款</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">待发货</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">已发货</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">已取消</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">已完成</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>



  </view>



</view>